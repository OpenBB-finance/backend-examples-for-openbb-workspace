          },
          {
            "headerName": "Name",
            "field": "name",
            "chartDataType": "category"
          },
          {
            "headerName": "Price",
            "field": "price",
            "chartDataType": "series"
          },
          {
            "headerName": "Market Cap",
            "field": "marketCap",
            "chartDataType": "series"
          },
          {
            "headerName": "Change",
            "field": "change",
            "chartDataType": "series"
          }
        ]
      }
    }
  },
  "jsonStockMock": {
    "name": "Json Mock Data",
